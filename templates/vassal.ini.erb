[uwsgi]
chdir=<%= @chdir %>
wsgi-file=<%= @wsgi_file %>
virtualenv=<%= @virtualenv %>
socket=/tmp/<%= @title %>.sock
daemonize=/var/log/uwsgi/<%= @title %>.log
processes=<%= @processes %>
master=<%= @master %>
vacuum=<%= @vacuum %>
single-interpreter = <%= @single_interpreter %>
enable-threads = <%= @enable_threads %>
